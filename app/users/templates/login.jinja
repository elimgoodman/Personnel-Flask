{% extends "base.jinja" %}
{% block content %}
    {% from "_form.jinja" import render_field %}
    <form method=post action="{{ url_for("users.login") }}">
      <dl>
        {{ render_field(form.email) }}
        {{ render_field(form.password) }}
      </dl>
      <p><input type=submit value="Log In">
    </form>
{% endblock %}

